		<xsd:documentation>Version 1.0                             Jim Oolman                                          Initial Draft</xsd:documentation>
	</xsd:annotation>
	<xsd:import namespace="http://www.crimnet.state.mn.us/MNJustice/1.0/ReferenceSchema" schemaLocation="PublicCriminalHistory_1.0_mnj.xsd"/>
	<xsd:element name="CriminalHistoryDump">
		<xsd:annotation>
			<xsd:documentation>This documents all the public criminal history data available at the time the file was created.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
					<xsd:element name="CriminalRecord" type="mnj:CriminalRecordType" maxOccurs="unbounded"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
</xsd:schema>
