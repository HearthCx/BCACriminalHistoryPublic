<xsd:schema targetNamespace="http://www.crimnet.state.mn.us/MNJustice/1.0/RuntimeSchema/PublicCriminalHistory/01.0" elementFormDefault="qualified" attributeFormDefault="unqualified" version="1.0" xmlns:mnj="http://www.crimnet.state.mn.us/MNJustice/1.0/ReferenceSchema" xmlns:mnr="http://www.crimnet.state.mn.us/MNJustice/1.0/RuntimeSchema/PublicCriminalHistory/01.0" xmlns:j="http://www.it.ojp.gov/jxdm/3.0.2" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
